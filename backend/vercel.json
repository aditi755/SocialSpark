{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server.js"
      }
    ],
    "functions": {
      "api/auth/**": {
        "memory": 1024,
        "maxDuration": 20
      }
    }
  }
  